<div class="container" >
    <div class="card-body card-block">
        <div class="login-content" >
            <div class="login-logo">
                
                <img src="images/icon/onda_logo.png" width="64" height="64" alt="Cool Admin" /> <strong>ONDA</strong></a>
            </div>
            <h3 class="title-3 m-b-30">
            <i class="zmdi zmdi-account-calendar"></i>Inspection Quotidienne</h3>
            <form action="/engineer/dialyInspection" method="POST">
                    <div class="login-form">
                            <div class="row form-group">
                                <div class="col col-md-3">
                                    <label for="text-input" class=" form-control-label">Date:</label>
                                </div>
                                <div class="col col-sm-3">
                                    <input type="datetime-local" required name="DATE" placeholder="Enter Date" class="form-control">
                                </div>
                            </div>
                            <div class="row form-group">
                                <div class="col col-md-3">
                                    <label for="text-input"  class=" form-control-label">Device:</label>
                                </div>
                                <div class="col col-sm-3">
                                       <select name="Device"required id="select" class="form-control">
                                        {{#each equipments}}
                                            <option value="{{this.Code}}">{{this.Name}} {{this.Code}} {{this.Department}}</option>
                                        {{/each}}
                                    </select>
                                </div>
                            </div>

                    </div>
                    <div class="top-campaign" >
                        <div class="table-responsive"  >
                                <table class="table table-top-campaign" >
                                <thead>
                                    <tr>
                                        <td> Questions</td>
                                        <td>Check</td>    
                                        <td></td>
                                    </tr>
                                    <tr>
                                        <td>1- Le calibrage de l'équipement est-il à jour ?</td>
                                    <td>
                                        <label class="au-checkbox">
                                            <input type="checkbox" name="Q1">
                                            <span class="au-checkmark"></span>
                                        </label>
                                    </td>
                                </tr>
                                <tr>
                                    <td>2- Y a-t-il des signes visibles d'usure ou de dommage ?</td>
                                    <td>
                                        <label class="au-checkbox">
                                            <input type="checkbox" name="Q2">
                                            <span class="au-checkmark"></span>
                                        </label>
                                    </td>
                                </tr>
                                <tr>
                                    <td>3- Les instructions et étiquettes de sécurité sont-elles clairement visibles ?</td>
                                    <td>
                                        <label class="au-checkbox">
                                            <input type="checkbox" name="Q3">
                                            <span class="au-checkmark"></span>
                                        </label>
                                    </td>
                                </tr>
                                <tr>
                                    <td>4- L'équipement fonctionne-t-il correctement sans erreurs ?</td>
                                    <td>
                                        <label class="au-checkbox">
                                            <input type="checkbox" name="Q4">
                                            <span class="au-checkmark"></span>
                                        </label>
                                    </td>
                                </tr>
                                <tr>
                                    <td>5- Tous les accessoires et pièces nécessaires sont-ils présents ?</td>
                                    <td>
                                        <label class="au-checkbox">
                                            <input type="checkbox" name="Q5">
                                            <span class="au-checkmark"></span>
                                        </label>
                                    </td>
                                </tr>
                                <tr>
                                    <td>6- L'équipement a-t-il été nettoyé et entretenu correctement ?</td>
                                    <td>
                                        <label class="au-checkbox">
                                            <input type="checkbox" name="Q6">
                                            <span class="au-checkmark"></span>
                                        </label>
                                    </td>
                                </tr>
                                <tr>
                                    <td>7- Les journaux et dossiers de maintenance sont-ils à jour ?</td>
                                    <td>
                                        <label class="au-checkbox">
                                            <input type="checkbox" name="Q7">
                                            <span class="au-checkmark"></span>
                                        </label>
                                    </td>
                                </tr>
                                <tr>
                                    <td>8- Y a-t-il des bruits ou des anomalies inhabituels provenant de l'équipement ?</td>
                                    <td>
                                        <label class="au-checkbox">
                                            <input type="checkbox" name="Q8">
                                            <span class="au-checkmark"></span>
                                        </label>
                                    </td>
                                </tr>
                            </thead>
                            <tr>
                                <td>
                                    <button class="au-btn au-btn-load" style="margin-left: 0;">Enregistrer</button>
                                </td>
                            </tr>
                        </table>
                    </div>
                </div>
            </form>
        </div>
    </div>
</div>