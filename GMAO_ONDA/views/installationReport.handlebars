<div class="container">
            <div class="card-body card-block">
                <div class="login-content">

                    <div class="login-logo">
                       
                        <img src="images/icon/onda_logo.png" width="64" height="64" alt="Cool Admin" /> <strong>ONDA</strong></a>
                    </div>
                    <h3 class="title-3 m-b-30">
                        <i class="zmdi zmdi-account-calendar"></i>Installation Report</h3>
                    <div class="card-body">
                        <div class="card-text"><strong>Date/Time:</strong> {{Date}} </div>
                        <div class="card-text"><strong>Device:</strong> {{ equipment.Name}}</div>
                        <div class="card-text"><strong>Modèle:</strong> {{equipment.Model}}</div>
                         <div class="card-text"><strong>Service:</strong> {{equipment.Department}}</div>
                        <div class="card-text"><strong>Emplacement:</strong> {{equipment.Location}}</div>
                    </div>
                    <div class="row">
                        <!-- table 1 DATA-->
                        <div class="col-lg-6">
                            <div class="top-campaign">
                                <h3 class="title-3 m-b-30">Details</h3>
                                <div class="table-responsive">
                                    <table class="table table-top-campaign">
                                        <thead>
                                            <tr>
                                                <td><strong>Note:  </strong><span style="color:#0056b3">{{equipment.Notes}}</span> </td>
                                                <td hidden>//</td>
                                            </tr>
                                            <tr>
                                                <td><strong>Série:  </strong> <span style="color:#0056b3">{{ equipment.SerialNumber}}</span></td>
                                                <td hidden>//</td>
                                            </tr>
                                            <tr>
                                                <td><strong>Fabricant:  </strong><span style="color:#0056b3"> {{ equipment.Manufacturer}}</span> </td>
                                                <td hidden>//</td>
                                            </tr>
                                            <tr>
                                                <td><strong>Agent:  </strong> <span style="color:#0056b3">  {{ equipment.Agent}}</span></td>
                                                <td hidden>//</td>
                                            </tr>  
                                            <tr>
                                                <td><strong>Equipement sous contrat n°:  </strong> <span style="color:#0056b3">  {{ equipment.Eqcontrat}}</span></td>
                                                <td hidden>//</td>
                                            </tr>
                                            <tr>
                                                <td><strong>Observation:  </strong> <span style="color:#0056b3">  {{ equipment.Observation}}</span></td>
                                                <td hidden>//</td>
                                            </tr> 
                                        </thead>
                                    </table>
                                </div>  
                            </div>
                        </div>
                        <!-- table 2 DATA-->
                        <div class="col-lg-6">
                            <div class="top-campaign">
                                <h3 class="title-3 m-b-30">Info d'achat</h3>
                                <div class="table-responsive">
                                    <table class="table table-top-campaign">
                                        <thead>
                                            <tr>
                                                <td><strong>Date d'arrivée: </strong>  <span style="color:#0056b3"> {{ equipment.ArrivalDate}}</span></td>
                                                <td hidden> //</td>
                                            </tr>
                                            <tr>
                                                <td><strong>Date d'installation: </strong> <span style="color:#0056b3"> {{ equipment.InstallationDate}}</span> </td>
                                                <td hidden>//</td>
                                            </tr>
                                            <tr>
                                                <td><strong>Date de garantie: </strong>  <span style="color:#0056b3"> {{equipment.WarrantyDate}}</span></td>
                                                <td hidden>//</td>
                                            </tr>
                                            <tr>
                                                <td><strong>Prix ​​d'achat: </strong> <span style="color:#0056b3"> {{ equipment.Cost}} L.E</span> </td>
                                                <td hidden>L.E</td>
                                            </tr>
                                            <tr>
                                                <td><strong>Horaire du PM: </strong>  <strong style="color:#0056b3">{{equipment.PM}}</strong> </td>
                                                <td hidden> //</td>
                                                
                                            </tr>   
                                        </thead>
                                    </table>
                                </div>   
                            </div>
                            <!-- END USER DATA-->
                        </div>

                    </div> 
                </div>
            </div> 
        </div>
        
    </div> 
    