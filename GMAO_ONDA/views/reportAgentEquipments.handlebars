<div class="main-content">
    <div class="section__content section__content--p30"> 
        <div class="container-fluid">
            <div class="table-data__tool">
                <h2> Agent/Fournisseur: {{ name }}</h2>
            </div>
            <div class="table-responsive m-b-40">
                <table class="table table-borderless table-data3">
                    <thead>
                        <tr>
                            <th>Code</th>
                            <th>Nom</th>
                            <th>Coût</th>
                            <th>Modèle</th>
                            <th>Numéro de Série</th>
                            <th>Fabricant</th>
                            <th>Emplacement</th>
                            <th>Date d'installation</th>
                            <th>Service</th>    
                        </tr>
                    </thead>
                   {{#if hasEquipments}} 
                    <tbody>
                     {{#each equipments}}
                        <tr>
                            <td>{{this.Code}}</td>
                            <td>{{this.Name}}</td>
                            <td>{{this.Cost}}</td>
                            <td>{{this.Model}}</td>
                            <td>{{this.SerialNumber}}</td>
                            <td>{{this.Manufacturer}}</td>
                            <td>{{this.Location}}</td>
                            <td>{{this.InstallationDate}}</td>
                            <td>{{this.Department}}</td>    
                        </tr>
                       {{/each}} 
                    </tbody>
                   {{/if}} 
                </table>
            </div>
        </div>
    </div>
</div>
        <!-- END PAGE CONTAINER-->  

